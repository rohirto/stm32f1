{
  "version": "0.2.0",
  "configurations": [
    
    {
      "cwd": "${workspaceRoot}",
      "executable": "${workspaceRoot}/build/${workspaceFolderBasename}.elf",
      "name": "STM32F1 Debug",
      "request": "launch",
      "type": "cortex-debug",
      "servertype": "openocd",
      "interface": "swd",
      "configFiles": [
        "/usr/share/openocd/scripts/interface/stlink-v2.cfg",
        "/usr/share/openocd/scripts/target/stm32f1x.cfg"  //Config for F1 Boards
      ],
      "searchDir": [],
      "runToEntryPoint": "main",
      "showDevDebugOutput": "raw",
    },
    {
      "name": "Cortex Debug",
      "cwd": "${workspaceRoot}",
      "executable": "${workspaceRoot}/build/${workspaceFolderBasename}.elf",
      "request": "launch",
      "type": "cortex-debug",
      "servertype": "openocd",
      "device": "STM32F103C8T6",
      "configFiles": [
        "interface/stlink.cfg",
        "target/stm32f1x.cfg"
      ],
      "preLaunchTask": "flash-openocd",
      "showDevDebugOutput": "raw",
      "svdFile": "${workspaceRoot}/debug-files/STM32F103.svd"
    }
  ]
}